<!DOCTYPE html>
<html>
  <head>
    <title>Findmyflock</title>
    <meta name="robots" content="noindex">

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
  </head>

  <body>
    <% if notice || alert %>
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
    <% end %>
    <%= yield %>
    <%= javascript_pack_tag 'google-autocomplete' %>


    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
    <%= javascript_pack_tag 'form-skills' %>
<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyA7K6g78F78NhFmIXvpYkICOIdeGrtdGIk&libraries=places" %>
  </body>
</html>
