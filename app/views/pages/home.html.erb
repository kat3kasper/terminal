<h1>Home page</h1>
<% if recruiter_signed_in? %>
  <li>
  <%= link_to('Logout Recruiter', destroy_recruiter_session_path, method: :delete) %>
  </li>
<% else %>
  <li>
  <%= link_to('Login Recruiter', new_recruiter_session_path)  %> |
  <%= link_to('Signup Recruiter', new_recruiter_registration_path)  %>
  </li>
<% end %>

<% if developer_signed_in? %>
  <li>
  <%= link_to('Logout Developer', destroy_developer_session_path, method: :delete) %>
  </li>
<% else %>
  <li>
  <%= link_to('Login Developer', new_developer_session_path)  %> |
  <%= link_to('Signup Developer', new_developer_registration_path)  %>
  </li>
<% end %>
